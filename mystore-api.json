{
  "openapi": "3.0.0",
  "info": {
    "title": "My Store Static API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://raw.githubusercontent.com/Fawaz-Mughni/dummyjson-openapi/refs/heads/main/mystore.json"
    }
  ],
  "paths": {
    "/mystore.json": {
      "get": {
        "summary": "Get static store products",
        "responses": {
          "200": {
            "description": "Static product list",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "products": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": { "type": "integer" },
                          "title": { "type": "string" },
                          "price": { "type": "number" }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
